include ./basic_navigator.jade

block content
  h1.
    Leaderboard
  div
    table.table.table-striped.table-hover
      thead
        tr
          th Level Name
          th Script Name
          th Time
        tbody
        - if (userIsLogged) {
        each entry, i in leaderboard
          tr
            td= entry.levelName
            td= entry.scriptName
            td= entry.time
        - }
