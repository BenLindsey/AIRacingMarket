include ./basic_navigator.jade

block content
  h1.
    Leaderboard
  div
    table.table.table-striped.table-hover
      thead
        tr
          th Script
          th Rating
        tbody
        - if (leaderboard.length) {
        each entry, i in leaderboard
          tr
            td= entry.name
            td= Math.round(entry.rating)
        - }
