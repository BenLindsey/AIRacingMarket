include ./basic_navigator.jade

block content
    script.
        var positions = ["First", "Second", "Third", "Fourth"];

    - if (scripts.length > 0) {
    table.table.table-striped.table-hover
      thead
        tr
          th Position
          th Script
        tbody
        each entry, i in scripts
          tr
            td= positions[i]
            td= entry.name
    - }